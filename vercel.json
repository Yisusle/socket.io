{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "headers": { "Location": "/chat" },
      "status": 301,
      "dest": "/chat"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.js"
    }
  ]
}

